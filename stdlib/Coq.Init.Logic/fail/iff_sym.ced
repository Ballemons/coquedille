module test.
data and (A : ★) (B : ★) : ★ =
  | conj : A ➔ B ➔ and.

iff : Π A : ★ . Π B : ★ . ★ = λ A : ★ . λ B : ★ . and ·(λ _ : A . B) ·(λ _ : B . A).

iff_sym : ∀ A : ★ . ∀ B : ★ . iff ·A ·B ➔ iff ·B ·A = Λ A : ★ . Λ B : ★ . λ H : iff ·A ·B . σ H @(λ _ : and ·(λ _ : A . B) ·(λ _ : B . A) . iff ·B ·A) {
  | conj H1 H2 ➔ conj ·(λ _ : B . A) ·(λ _ : A . B) H2 H1 
 }.


